                }
                return;
            }

            // Refresh file folder
            if (data.action == 'refresh') { p13folderup(9999); return; }

            if (data.path != null) {
                data.path = data.path.replace(/\//g, '\\');
                if ((p13filetree != null) && (data.path == p13filetree.path)) {
                    // This is an update to the same folder
                    var checkedNames = p13getCheckedNames();
                    p13filetree = data;
